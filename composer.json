{
  "name": "make-it-app/laravel-limit-user-logins",
  "description": "Limit logged In user devices",
  "keywords": [
    "laravel",
    "limit", "user", "login", "logins"
  ],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/make-it-app/laravel-limit-user-logins/issues",
    "source": "https://github.com/make-it-app/laravel-limit-user-logins"
  },
  "authors": [
    {
      "name": "Stanislav Kabin",
      "email": "ioffline@make-it.app"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "laravel/framework": "^10.10"
  },
  "require-dev": {
    "laravel/pint": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "MakeIT\\LoginLimiter\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.x-dev"
    },
    "laravel": {
      "providers": [
        "MakeIT\\LoginLimiter\\LoginLimiterServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
